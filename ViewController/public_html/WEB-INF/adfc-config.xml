<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="Login">
    <page>/Pages/Login.jspx</page>
  </view>
  <view id="Registration">
    <page>/Pages/Registration.jspx</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>Login</from-activity-id>
    <control-flow-case id="__2">
      <from-outcome>Registration</from-outcome>
      <to-activity-id>Registration</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__3">
    <managed-bean-name>Login</managed-bean-name>
    <managed-bean-class>Bean.Login</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__4">
    <managed-bean-name>Registration</managed-bean-name>
    <managed-bean-class>Bean.Registration</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</adfc-config>
